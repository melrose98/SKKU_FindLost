<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.skku.map.skku_findlost">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="edu.skku.map.skku_findlost.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.skku.map.skku_findlost.SignupActivity"/>
        <activity android:name="edu.skku.map.skku_findlost.MenuActivity"/>
        <activity android:name="edu.skku.map.skku_findlost.ListLostActivity"/>
        <activity android:name="edu.skku.map.skku_findlost.ListFoundActivity"/>
        <activity android:name="edu.skku.map.skku_findlost.ReportLostActivity"/>
        <activity android:name="edu.skku.map.skku_findlost.ReportFoundActivity"/>

    </application>

</manifest>